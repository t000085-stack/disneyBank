{
  "expo": {
    "name": "Bankappp",
    "slug": "Bankappp",
    "version": "1.0.0",
    "scheme": "bankappp",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
